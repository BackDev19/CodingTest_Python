    "from sys import stdin\n",
    "stdin = open(\"./input/input9.txt\", \"rt\")\n",
    "input = stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "for i in range(1, n):\n",
    "    arr[i] = max(arr[i], arr[i-1] + arr[i])\n",
    "\n",
    "print(max(arr))"
   ]
