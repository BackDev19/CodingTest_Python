   "source": [
    "n = int(input())\n",
    "dp = [0 for _ in range(31)]\n",
    "dp[2] = 3\n",
    "for i in range(4,n+1) :\n",
    "    if i%2 == 0 :\n",
    "        dp[i] = dp[i-2] * 3 + sum(dp[:i-2]) * 2 + 2\n",
    "    else :\n",
    "        dp[i] = 0\n",
    "print(dp[n])"
   ]
  }
