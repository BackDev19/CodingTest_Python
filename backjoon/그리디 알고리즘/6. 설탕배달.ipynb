    "import sys\n",
    "sys.stdin = open(\"./input/input6.txt\", \"rt\")\n",
    "\n",
    "sugar = int(sys.stdin.readline())\n",
    "bag = 0\n",
    "\n",
    "while sugar >= 0:\n",
    "    if sugar % 5 == 0:\n",
    "        bag += sugar // 5\n",
    "        print(bag)\n",
    "        break\n",
    "    sugar -= 3\n",
    "    bag += 1\n",
    "else:\n",
    "    print(-1)\n"
   ]
  }
