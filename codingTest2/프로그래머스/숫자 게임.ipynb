    "def solution(A, B):\n",
    "    \n",
    "    A.sort(reverse = True)\n",
    "    B.sort(reverse = True)\n",
    "    \n",
    "    answer = 0\n",
    "    idx = 0\n",
    "    for i in range(len(A)):\n",
    "        if A[i] < B[idx]:\n",
    "            answer += 1\n",
    "            idx += 1\n",
    "    \n",
    "    return answer"
   ]
