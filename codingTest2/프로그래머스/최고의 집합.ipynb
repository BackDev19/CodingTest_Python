    "def solution(n, s):\n",
    "    answer = []\n",
    "\n",
    "    if s < n:\n",
    "        return [-1]\n",
    "\n",
    "    for i in range(n):\n",
    "        answer.append(s // n)\n",
    "    \n",
    "    for i in range(s % n):\n",
    "        answer[-1-i] += 1\n",
    "\n",
    "    return answer"
   ]
  }
