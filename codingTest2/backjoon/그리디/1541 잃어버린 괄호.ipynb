    "import sys\n",
    "from collections import deque\n",
    "sys.stdin = open(\"input.txt\", \"rt\")\n",
    "input = sys.stdin.readline\n",
    "\n",
    "arr = input().split(\"-\")\n",
    "\n",
    "ans = 0\n",
    "for i in arr[0].split(\"+\"):\n",
    "    ans += int(i)\n",
    "\n",
    "for i in arr[1:]:\n",
    "    for j in i.split(\"+\"):\n",
    "        ans -= int(j)\n",
    "\n",
    "print(ans)"
   ]
  }
